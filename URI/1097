#include <stdio.h>
int main()
{
    int i,j,n;
    for(i=1;i<=9;i=i+2)
    {    if(i==1)
            {j=7;
        n=j-2;}
        if(i==3)
            {j=9;
        n=j-2;}
        if(i==5)
            {j=11;
        n=j-2;}
        if(i==7)
            {j=13;
        n=j-2;}
        if(i==9)
            {j=15;
        n=j-2;}
        for(j;j>=n;j--)
        {
            printf("I=%d J=%d\n",i,j);
        }



    }
    return 0;

}
